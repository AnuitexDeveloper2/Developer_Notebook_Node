    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "randomstring": "^1.2.1",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
